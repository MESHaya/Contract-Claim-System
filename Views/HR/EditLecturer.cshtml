@model IEnumerable<ClaimSystem.Models.Lecturer>

<h2>Edit Lecturer</h2>

<ul>
    @foreach (var lecturer in Model)
    {
        <li>
            <a href="@Url.Action("EditLecturerDetails", "HR", new { lecturerId = lecturer.LecturerId })">
                @lecturer.Name
            </a>
        </li>
    }
</ul>
